   ],
   "source": [
    "import numpy as np\n",
    "def inverse_matrix(matrix):\n",
    "  return np.linalg.inv(matrix)\n",
    "\n",
    "print(inverse_matrix([[1,2],[3,4]]))"
   ]
  }
