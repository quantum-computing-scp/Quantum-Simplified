   "source": [
    "**Input**: A complex matrix\n",
    "\n",
    "**Goal**: Return the conjugate transpose of the following matrix"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "68bedb0c",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[[1.-5.j 3.+6.j]\n",
      " [2.-0.j 0.-4.j]]\n"
     ]
    }
   ],
   "source": [
    "import numpy as np\n",
    "def conjugate_transpose_matrix(matrix):\n",
    "  np_matrix = np.array(matrix)\n",
    "  return np_matrix.T.conjugate()\n",
    "\n",
    "print(conjugate_transpose_matrix([[1+5j, 2],[3-6j, 4j]]))"
   ]
  }
