   "source": [
    "import numpy as np\n",
    "\n",
    "U = np.array([[1,1],[1,-1]])/np.sqrt(2)\n",
    "\n",
    "def apply_u(state):\n",
    "  return state@U\n",
    "\n",
    "ket_0 = np.array([1,0])\n",
    "ket_1 = np.array([0,1])\n",
    "\n",
    "print(apply_u(ket_0))\n",
    "print(apply_u(ket_1))"
   ]
  }
