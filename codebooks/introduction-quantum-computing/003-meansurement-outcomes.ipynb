    "import numpy as np\n",
    "\n",
    "def measure_state(state, num_meas):\n",
    "  P_0 = np.abs(state[0])**2\n",
    "  P_1 = np.abs(state[1])**2\n",
    "  samples = np.random.choice([0,1], num_meas, True, [P_0, P_1])\n",
    "  return samples\n",
    "\n",
    "state = np.array([0.8, 0.6])\n",
    "num_meas = 10\n",
    "\n",
    "print(measure_state(state, num_meas))"
   ]
  }
